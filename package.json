{
  "name": "cpreact",
  "version": "0.5.4",
  "description": "a lightweight react",
  "main": "lib/cpreact.js",
  "types": "lib/cpreact.d.ts",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "node script/build.js",
    "test": "npm run test:karma NODE_ENV=production",
    "test:mocha": "mocha --recursive --require babel-register test/shared",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:mocha:watch": "npm run test:mocha -- watch",
    "test:karma:watch": "npm run test:karma -- no-single-run"
  },
