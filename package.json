  "author": "muyunyun(muyy95@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MuYunyun/cpreact/issues"
  },
  "homepage": "https://github.com/MuYunyun/cpreact#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
